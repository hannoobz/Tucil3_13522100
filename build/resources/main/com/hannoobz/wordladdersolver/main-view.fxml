<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="320" maxWidth="400" minHeight="320" minWidth="400" prefHeight="320.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.hannoobz.wordladdersolver.MainController">
   <children>
      <TextField layoutX="122.0" layoutY="55.0" fx:id="start"/>
      <Text layoutX="190.0" layoutY="44.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Start" />
      <TextField layoutX="123.0" layoutY="122.0" fx:id="target" />
      <Text layoutX="186.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Target" />
      <Button layoutX="269.0" layoutY="197.0" mnemonicParsing="false" text="Solve" onAction="#clickSolve"/>
      <Button layoutX="244.0" layoutY="237.0" mnemonicParsing="false" text="Dictionary" />
      <VBox layoutX="104.0" layoutY="197.0" prefHeight="63.0" prefWidth="100.0">
         <children>
            <RadioButton mnemonicParsing="false" text="GreedyBFS">
               <toggleGroup>
                  <ToggleGroup fx:id="group" />
               </toggleGroup>
               <padding>
                  <Insets bottom="3.0" />
               </padding>
            </RadioButton>
            <RadioButton mnemonicParsing="false" text="UCS" toggleGroup="$group">
               <padding>
                  <Insets bottom="3.0" />
               </padding>
            </RadioButton>
            <RadioButton mnemonicParsing="false" text="A*" toggleGroup="$group">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="3.0" />
               </padding>
            </RadioButton>
         </children>
      </VBox>
   </children>
</AnchorPane>
